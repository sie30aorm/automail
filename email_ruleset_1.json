      {
         "RULESET":"Cliente: referencias a casos previos",
         "RULES":[
            {
               "RULE":"CASE:Caso referenciado en el asunto",
               "IF":[
                  {
                     "FIELD":"#RULE",
                     "COND":"is_empty"
                  },
                  {
                     "FIELD":"Asunto",
                     "COND":"contains_case_ref",
                     "AS":"@CASE"
                  }
               ],
               "THEN":[
                  {
                     "FIELD":"#RULE",
                     "SET_VALUE":"@RULE"
                  },
                  {
                     "FIELD":"#CASE",
                     "SET_VALUE":"@CASE"
                  }
               ]
            },
            {
               "RULE":"CASE:Caso referenciado en el cuerpo",
               "IF":[
                  {
                     "FIELD":"#RULE",
                     "COND":"is_empty"
                  },
                  {
                     "FIELD":"Cuerpo",
                     "COND":"contains_case_ref"
                  }
               ],
               "THEN":[
                  {
                     "FIELD":"#RULE",
                     "SET_VALUE":"@RULE"
                  },
                  {
                     "FIELD":"#TIENE_CASO",
                     "SET_VALUE":"SI"
                  }
               ]
            }
         ]
      },
      {
         "RULESET":"Cliente: Nuevas Solicitudes",
         "GLOBAL-IF":[
            {
               "FIELD":"Tecnología",
               "COND":"contain_aprox",
               "MATCH_VALUES":[
                  "movil"
               ]
            }
         ],
         "GLOBAL-THEN":[
            {
               "FIELD":"#TIPIFICACION CORRECTA",
               "SET_VALUE":"global value is set"
            }
         ],
         "RULES":[
            {
               "RULE":"SOL:Cambio de codigo",
               "IF":[
                  {
                     "FIELD":"#RULE",
                     "COND":"is_empty"
                  },
                  {
                     "FIELD":"Asunto",
                     "COND":"match_regexp",
                     "MATCH_VALUES":[
                        "cambi.*c.digo"
                     ]
                  }
               ],
               "THEN":[
                  {
                     "FIELD":"#RULE",
                     "SET_VALUE":"@RULE"
                  },
                  {
                     "FIELD":"#ACCION",
                     "SET_VALUE":"CASE_OPEN"
                  },
                  {
                     "FIELD":"#INTENCION",
                     "SET_VALUE":"#MOVIL_SOL_CAMBIO_CODIGO"
                  }
               ]
            }
         ]
